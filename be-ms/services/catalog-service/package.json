{
  "name": "catalog-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p .",
    "postinstall": "npm run build",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "proto:generate": "npm run proto:clean && npm run proto:ts",
    "proto:clean": "rimraf src/grpc/generated/* || true",
    "proto:ts": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/grpc/generated --ts_proto_opt=outputServices=grpc-js,useOptionals=messages,esModuleInterop=true,forceLong=string --proto_path=src/grpc/proto src/grpc/proto/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "grpc-tools": "^1.13.0",
    "nodemon": "^3.1.10",
    "protoc-gen-ts": "^0.8.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.7.7",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "google-protobuf": "^4.0.0",
    "mongoose": "^8.16.5",
    "multer": "^2.0.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.1.3"
  }
}
